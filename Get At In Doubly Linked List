void getAt(int index,int len){
	if(len == 0){
		cout << "List is empty" << endl ; 
	}
	else if(index > len ){
 		cout << "Index out of range" << endl ; 
	}
	else{int i = 0 ;
	     while(i != index+1){
	    	  if(i==index){
	    		cout << head -> data ; 
	    		i++ ; 
			   }
			  else{
			  	head = head -> next ;
			    i++ ;
			   } 
	    }
   }
}
